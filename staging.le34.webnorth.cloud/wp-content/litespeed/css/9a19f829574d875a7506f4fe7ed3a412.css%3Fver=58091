@-webkit-keyframes goIn {
    0% {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        opacity: 0
    }

    50% {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }

    to {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }
}

@keyframes goIn {
    0% {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        opacity: 0
    }

    50% {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }

    to {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }
}

@-webkit-keyframes goOut {
    0% {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }

    50% {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        display: none;
        opacity: 0
    }

    to {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        opacity: 0;
        display: none
    }
}

@keyframes goOut {
    0% {
        -webkit-transform: translateX(0);
        transform: translate(0);
        opacity: 1
    }

    50% {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        display: none;
        opacity: 0
    }

    to {
        -webkit-transform: translateX(-30px);
        transform: translate(-30px);
        opacity: 0;
        display: none
    }
}



